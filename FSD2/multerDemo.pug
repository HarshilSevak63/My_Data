html 
 head 
  title Multer-Demo 
 body
  h1 For Single File:-
  form(action="/upload" method='post',enctype='multipart/form-data')
   label(for="myfile") Select File: 
   input(type="file",name='myfile', id='myfile')
   br
   br
   input(type='Submit',value='Upload')
   br
   br
   br
 h1 For Multiple File:-
 form(action="/uploadm" method='post',enctype='multipart/form-data')
   label(for="myfilem") Select File: 
   input(type="file",accept='.jpg,.png',name='myfilem', id='myfilem',multiple)
   br
   br
   input(type='Submit',value='Upload')